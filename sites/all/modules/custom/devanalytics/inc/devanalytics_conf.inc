    function resolve_State($states,$stcds,$city) {
    	for ($i = 0; $i < count($states); $i++) {
            $tmp = $states[$i];
            if ($tmp['city'] == $city) {
            	$st = $tmp['state'];
            	foreach ($stcds as $cd => $state) {
            		if ($state == $st) {
            			return $cd;
            		}
            	}
            }
        }
    }
